export PATH="/usr/local/bin:/usr/local/sbin:$PATH:/usr/libexec:/usr/local/vfuse/"
HOMEBREW_NO_ANALYTICS=1
export PS1="\A \u @ \W \\$  "
export HOMEBREW_GITHUB_API_TOKEN=[TOKEN]

alias ll='ls -lahG'
alias home='cd ~'
alias up='cd ..'
alias h='history'

alias mv='mv -i'
alias cp='cp -i'
alias rm='rm -i'
alias df='df -h'
alias du='du -h'
alias mkdir='mkdir -p'

alias pdw='pwd'
